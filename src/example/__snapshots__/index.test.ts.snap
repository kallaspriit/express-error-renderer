  // throws an error
  app.get(\\"/throw-error\\", (_request, _response, _next) => {
    throw new Error(\\"Thrown error message\\");
  });

  // forwards an error
  app.get(\\"/next-error\\", (_request, _response, next) => {
    next(new Error(\\"Forwarded error message\\"));
